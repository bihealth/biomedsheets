    "title": "Generic Study Example",
    "description": "Example biomedical sheet for generic study",

    "extraInfoDefs": {
        "bioEntity": {
            "replicate": { "$ref": "file://file_types.json#/extraInfoDefs/template/replicate" },
            "taxon": { "$ref": "file://file_types.json#/extraInfoDefs/template/taxon" },
            "group": {
                "docs": "Group of the specimen",
                "key": "group",
                "type": "enum",
                "choices": [ "control", "case1", "case2", "case3" ]
            }
        },
        "bioSample": {
            "facsDate": {
                "docs": "Date of sample FACS processing",
                "key": "facsDate",
                "type": "string",
                "pattern": "^[0-9]{4}-[1-9][0-9]-[1-9][0-9]$"
            },
            "treatments": {
                "docs": "Treatments of sample",
                "key": "treatments",
                "type": "enum",
                "choices": [ "fresh", "frozen", "other" ]
            },
            "medium": {
                "docs": "Medium of the sample",
                "key": "medium",
                "type": "enum",
                "choices": [ "water", "ethanol" ]
            },
            "cellType": {
                "docs": "Cell type in this experiment",
                "key": "cellType",
                "type": "enum",
                "choices": [ "lymphocyte", "monocyte", "basophil", "neutrophil" ]
            }
        },
        "ngsLibrary": {
            "kitType": { "$ref": "file://file_types.json#/extraInfoDefs/template/kitType" },
            "concentration": {
                "docs": "Concentration of the library",
                "key": "concentration",
                "type": "number",
                "unit": "ng/ul"
            }
        }
    },

